#!/usr/bin/bash

#generated by gen-scripts.r defined by definitions.r

usage()
{
cat <<EOF

usage: $0 options

Lists all invoices.

options:
-f Response format: json, csv, html, xml (optional) 

EOF

}

FVAL="json"


while getopts hf: OPTION
do
	case $OPTION in
	h)
	    usage
	    exit 1
	    ;;
		f)
			FVAL=$OPTARG
			;;
	?)
	    usage
	    exit
	    ;;
    esac
done


curl -k \
     -u `cat .invfpwd`:x \
     -d "" \
     "https://www.cebelca.biz/API?_r=invoice-sent-o&_m=select-all&_f=${FVAL}"